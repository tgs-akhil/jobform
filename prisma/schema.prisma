// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Form {
  id              String    @id @default(uuid())
  name            String    @unique
  minSalary       Int
  maxSalary       Int
  experience      Float
  rating          Float
  minExpectedSal  Int
  maxExpectedSal  Int
  lastSalary      Int
  hiringFee       Float
  evalScore       Int
  skills          String
  startDate       DateTime
  endDate         DateTime
}

model Shopping{
  id Int @id @default(autoincrement())
  date String
  budget BigInt
}